est store baseline2
reghdfe `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' fed vix industr reer if em==0 , absorb(ifs ym) vce(cluster ifs)
est store baseline3
xtreg `x' gprc if em==0 , fe
est store baseline4
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' if em==0 , fe
est store baseline5
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' fed vix industr reer if em==0 , fe
est store baseline6


}
