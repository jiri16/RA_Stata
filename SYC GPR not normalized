est store baseline3
xtreg `x' gprc  , fe
est store baseline4
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x'  , fe
est store baseline5
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' fed vix industr reer  , fe
est store baseline6


}
