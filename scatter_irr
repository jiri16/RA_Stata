#net

graph twoway (scatter irr_1 net_flow_1 if region==1, mlabel(countrycode)) (scatter irr_1 net_flow_1 if region==2, mlabel(countrycode)) (scatter irr_1 net_flow_1 if region==3, mlabel(countrycode)), legend(label(1 Europe) label(2 Asia) label(3 Latin America)) title("Real rate of return vs Total Net Flows (1980-2000)")

graph twoway (scatter irr_2 net_flow_2 if region==1, mlabel(countrycode)) (scatter irr_2 net_flow_2 if region==2, mlabel(countrycode)) (scatter irr_2 net_flow_2 if region==3, mlabel(countrycode)), legend(label(1 Europe) label(2 Asia) label(3 Latin America)) title("Real rate of return vs Total Net Flows (2000-2010)")

graph twoway (scatter irr_3 net_flow_3 if region==1, mlabel(countrycode)) (scatter irr_3 net_flow_3 if region==2, mlabel(countrycode)) (scatter irr_3 net_flow_3 if region==3, mlabel(countrycode)), legend(label(1 Europe) label(2 Asia) label(3 Latin America)) title("Real rate of return vs Total Net Flows (2010-2020)")

