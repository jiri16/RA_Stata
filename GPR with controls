est store baseline2
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' fed vix , fe
est store baseline3
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' indprod inf st_ir , fe
est store baseline4
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' fed vix indprod inf st_ir , fe
est store baseline5


}
