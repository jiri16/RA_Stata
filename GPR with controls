est store baseline2
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' fed vix, fe
est store baseline3
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' gr_indprod gr_inf gr_st_ir gr_er gr_msci, fe
est store baseline4
xtreg `x' gprc l1.`x' l2.`x' l3.`x' l4.`x' fed vix gr_indprod gr_inf gr_st_ir gr_er gr_msci , fe
est store baseline5


}
