foreach x in go_sov {

xtreg `x' l1savgnatshock l1savgpolshock l1savgrevshock l1savgtershock l1.`x' l2.`x' l3.`x' l4.`x' , vce(cluster ifs)

est store baseline1
xtreg `x' l1savgnatshock l1savgpolshock l1savgrevshock l1savgtershock usgdp_gr usffr usdollar comm vix l1.`x' l2.`x' l3.`x' l4.`x' , vce(cluster ifs) 
est store baseline2 
xtreg `x' l1savgnatshock l1savgpolshock l1savgrevshock l1savgtershock usgdp_gr usffr usdollar comm vix gdp_gr cpi cb_rate stock_gr fx_gr l1.`x' l2.`x' l3.`x' l4.`x' , vce(cluster ifs)
est store baseline3
xtreg `x' l1savgnatshock l1savgpolshock l1savgrevshock l1savgtershock usgdp_gr usffr usdollar comm vix gdp_gr cpi cb_rate stock_gr fx_gr l1.`x' l2.`x' l3.`x' l4.`x' , vce(cluster ifs) 
est store baseline4

}
esttab baseline*, b(%9.3f) se(%9.3f) star(* 0.10 ** 0.05 *** 0.01) stats(N r2_o)
esttab baseline* using "go_sov.csv", plain replace b(%9.5f) se(%9.5f) star(* 0.10 ** 0.05 *** 0.01) stats(N r2_o)
